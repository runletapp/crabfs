#!/bin/bash
PATH=./node_modules/.bin:$PATH

function generateProtos {
    protoc -I ./protobuf ./protobuf/*.proto --go_out=plugins=grpc:./protos
}

function help {
    echo "$0 <task> <args>"
    echo "Tasks:"
    compgen -A function | cat -n
}

TIMEFORMAT="Task completed in %3lR"
time ${@:-default}
